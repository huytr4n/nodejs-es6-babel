{
    "extends": "eslint:recommended",
    "env": {
        "browser": true,
        "node": true
    },
    "plugins": ["react"],
    "rules": {
        "no-console": 0,
        "new-cap": 0,
        "strict": 0,
        "no-underscore-dangle": 0,
        "no-use-before-define": 0,
        "eol-last": 0,
        "jsx-quotes": [2, "prefer-single"]
    }
}
